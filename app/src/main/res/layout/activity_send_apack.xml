<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SendApack">

    <RelativeLayout
        android:id="@+id/frame_56"
        android:layout_width="412dp"
        android:layout_height="94dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="#FFFFFF"
        android:clipToOutline="true"
        android:elevation="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <RelativeLayout
            android:id="@+id/top"
            android:layout_width="411dp"
            android:layout_height="21dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="false"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="58dp"
            android:layout_marginEnd="0dp"
            android:layout_marginBottom="15dp">

            <Button
                android:id="@+id/button3"
                android:layout_width="39dp"
                android:layout_height="31dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="0dp"
                android:layout_marginTop="0dp"
                android:layout_marginEnd="371dp"
                android:layout_marginBottom="0dp"
                android:backgroundTint="#00FFFFFF"
                app:icon="@drawable/img_3"
                app:iconGravity="textEnd"
                app:iconSize="24sp"
                app:iconTint="#0560FA"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="140dp"
                android:layout_marginTop="0dp"
                android:layout_marginEnd="140dp"
                android:layout_marginBottom="0dp"
                android:fontFamily="@font/roboto_medium"
                android:text="Send a package"
                android:textAlignment="center"
                android:textColor="#A7A7A7"
                android:textSize="16sp" />


        </RelativeLayout>
    </RelativeLayout>

    <Button
        android:id="@+id/btnReport"
        android:layout_width="158dp"
        android:layout_height="wrap_content"
        android:backgroundTint="#FFFFFF"
        android:text="Report"
        android:textColor="#0560FA"
        app:cornerRadius="6dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.114"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frame_56"
        app:layout_constraintVertical_bias="0.886"
        app:strokeColor="#0560FA"
        app:strokeWidth="1dp" />

    <Button
        android:id="@+id/btnSuc"
        android:layout_width="158dp"
        android:layout_height="wrap_content"
        android:backgroundTint="#0560FA"
        android:text="Successful"
        app:cornerRadius="6dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.875"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.901"
        app:strokeColor="#0560FA"
        app:strokeWidth="1dp" />
</androidx.constraintlayout.widget.ConstraintLayout>